            (0, _react1.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_AwsCostMetricsPanel.AwsCostMetricsPanel, {
                ...defaultProps
            }));
            const filterButton = _react1.screen.getByRole('button', {
                name: /filters/i
            });
            _react1.fireEvent.click(filterButton);
            // Apply a filter first
            const granularitySelect = _react1.screen.getByDisplayValue('Daily');
            _react1.fireEvent.change(granularitySelect, {
                target: {
                    value: 'weekly'
                }
            });
            // Clear filters button should appear
            expect(_react1.screen.getByText('Clear all filters')).toBeInTheDocument();
        });
        it('clears all filters when clear button is clicked', async ()=>{
            const mockOnFiltersChange = jest.fn();
            (0, _react1.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_AwsCostMetricsPanel.AwsCostMetricsPanel, {
                ...defaultProps,
                onFiltersChange: mockOnFiltersChange
            }));
            const filterButton = _react1.screen.getByRole('button', {
                name: /filters/i
            });
            _react1.fireEvent.click(filterButton);
            // Apply a filter first
            const granularitySelect = _react1.screen.getByDisplayValue('Daily');
            _react1.fireEvent.change(granularitySelect, {
                target: {
                    value: 'weekly'
                }
            });
            // Click clear filters
            const clearButton = _react1.screen.getByText('Clear all filters');
            _react1.fireEvent.click(clearButton);
            await (0, _react1.waitFor)(()=>{
                expect(mockOnFiltersChange).toHaveBeenCalledWith({});
            });
        });
        it('updates cost range filters correctly', async ()=>{
            const mockOnFiltersChange = jest.fn();
            (0, _react1.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_AwsCostMetricsPanel.AwsCostMetricsPanel, {
                ...defaultProps,
                onFiltersChange: mockOnFiltersChange
            }));
            const filterButton = _react1.screen.getByRole('button', {
                name: /filters/i
            });
            _react1.fireEvent.click(filterButton);
            // Set min cost
            const minInput = _react1.screen.getByPlaceholderText('Min');
            _react1.fireEvent.change(minInput, {
                target: {
                    value: '100'
                }
            });
            await (0, _react1.waitFor)(()=>{
                expect(mockOnFiltersChange).toHaveBeenCalledWith(expect.objectContaining({
                    costThreshold: expect.objectContaining({
                        min: 100
                    })
                }));
            });
            // Set max cost
            const maxInput = _react1.screen.getByPlaceholderText('Max');
            _react1.fireEvent.change(maxInput, {
                target: {
                    value: '2000'
                }
            });
            await (0, _react1.waitFor)(()=>{
                expect(mockOnFiltersChange).toHaveBeenCalledWith(expect.objectContaining({
                    costThreshold: expect.objectContaining({
                        max: 2000
                    })
                }));
            });
        });
    });
    describe('Accessibility', ()=>{
        it('has proper ARIA labels and roles', ()=>{
            (0, _react1.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_AwsCostMetricsPanel.AwsCostMetricsPanel, {
                ...defaultProps
            }));
            // Check navigation structure
            const tablist = _react1.screen.getByRole('navigation');
            expect(tablist).toBeInTheDocument();
            // Check buttons are accessible
            const buttons = _react1.screen.getAllByRole('button');
            expect(buttons.length).toBeGreaterThan(0);
            // Each button should be focusable
            buttons.forEach((button)=>{
                expect(button).not.toHaveAttribute('aria-disabled');
            });
        });
        it('filter controls have proper labels', ()=>{
            (0, _react1.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_AwsCostMetricsPanel.AwsCostMetricsPanel, {
                ...defaultProps
            }));
            const filterButton = _react1.screen.getByRole('button', {
                name: /filters/i
            });
            _react1.fireEvent.click(filterButton);
            // Check form labels using more specific selectors to avoid conflicts
            expect(_react1.screen.getByLabelText(/services/i)).toBeInTheDocument();
            expect(_react1.screen.getByLabelText(/regions/i)).toBeInTheDocument();
            expect(_react1.screen.getByLabelText(/granularity/i)).toBeInTheDocument();
            expect(_react1.screen.getByLabelText(/chart type/i)).toBeInTheDocument();
            expect(_react1.screen.getByText('Cost Range ($)')).toBeInTheDocument();
        });
    });
});

