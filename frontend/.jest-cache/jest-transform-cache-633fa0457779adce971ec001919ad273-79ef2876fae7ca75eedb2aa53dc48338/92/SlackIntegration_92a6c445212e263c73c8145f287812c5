                }),
                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("label", {
                            className: "block text-sm font-medium text-gray-700 dark:text-gray-300 mb-2",
                            children: "Alert Types"
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                            className: "grid grid-cols-2 gap-2",
                            children: alertTypeOptions.map((option)=>/*#__PURE__*/ (0, _jsxruntime.jsxs)("label", {
                                    className: "flex items-center",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("input", {
                                            type: "checkbox",
                                            checked: formData.alertTypes.includes(option.value),
                                            onChange: (e)=>{
                                                const newAlertTypes = e.target.checked ? [
                                                    ...formData.alertTypes,
                                                    option.value
                                                ] : formData.alertTypes.filter((t)=>t !== option.value);
                                                setFormData((prev)=>({
                                                        ...prev,
                                                        alertTypes: newAlertTypes
                                                    }));
                                            },
                                            className: "mr-2"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            className: "text-sm",
                                            children: option.label
                                        })
                                    ]
                                }, option.value))
                        })
                    ]
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("label", {
                            className: "block text-sm font-medium text-gray-700 dark:text-gray-300 mb-2",
                            children: "Minimum Threshold"
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("select", {
                            value: formData.threshold,
                            onChange: (e)=>setFormData((prev)=>({
                                        ...prev,
                                        threshold: e.target.value
                                    })),
                            className: "w-full px-3 py-2 border border-gray-300 dark:border-gray-600 rounded-lg focus:outline-none focus:ring-2 focus:ring-blue-500 bg-white dark:bg-gray-800",
                            children: thresholdOptions.map((option)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                    value: option.value,
                                    children: option.label
                                }, option.value))
                        })
                    ]
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("label", {
                            className: "block text-sm font-medium text-gray-700 dark:text-gray-300 mb-2",
                            children: "Custom Message Template"
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("textarea", {
                            value: formData.customMessage,
                            onChange: (e)=>setFormData((prev)=>({
                                        ...prev,
                                        customMessage: e.target.value
                                    })),
                            placeholder: "OpsSight Alert: {alert_type} - {message}",
                            rows: 3,
                            className: "w-full px-3 py-2 border border-gray-300 dark:border-gray-600 rounded-lg focus:outline-none focus:ring-2 focus:ring-blue-500 bg-white dark:bg-gray-800"
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("p", {
                            className: "text-xs text-gray-500 dark:text-gray-400 mt-1",
                            children: [
                                "Available variables: ",
                                '{alert_type}',
                                ", ",
                                '{message}',
                                ", ",
                                '{timestamp}',
                                ", ",
                                '{severity}'
                            ]
                        })
                    ]
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    className: "flex gap-3",
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.Button, {
                            type: "submit",
                            className: "flex-1",
                            children: config ? 'Update Configuration' : 'Create Configuration'
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.Button, {
                            type: "button",
                            onClick: onClose,
                            variant: "outline",
                            children: "Cancel"
                        })
                    ]
                })
            ]
        })
    });
};
const _default = SlackIntegration;

