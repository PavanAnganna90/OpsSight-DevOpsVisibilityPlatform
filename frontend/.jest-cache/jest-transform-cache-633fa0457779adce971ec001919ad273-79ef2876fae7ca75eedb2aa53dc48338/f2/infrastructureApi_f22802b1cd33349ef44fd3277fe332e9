async function fetchInfrastructure() {
    const res = await fetch('/api/infrastructure');
    if (!res.ok) throw new Error('Failed to fetch infrastructure');
    return res.json();
}
async function restartResource(id) {
    const res = await fetch(`/api/infrastructure/${id}/restart`, {
        method: 'POST'
    });
    if (!res.ok) throw new Error('Failed to restart resource');
    return res.json();
}
async function deleteResource(id) {
    const res = await fetch(`/api/infrastructure/${id}/delete`, {
        method: 'POST'
    });
    if (!res.ok) throw new Error('Failed to delete resource');
    return res.json();
}

