      cov_26jo2lsnbq().b[19][0]++;
      cov_26jo2lsnbq().s[86]++;
      currentKey.current = key;
    } else
    /* istanbul ignore next */
    {
      cov_26jo2lsnbq().b[19][1]++;
    }
    cov_26jo2lsnbq().s[87]++;
    setIsLoading(true);
    const initialValue =
    /* istanbul ignore next */
    (cov_26jo2lsnbq().s[88]++, readValue());
    /* istanbul ignore next */
    cov_26jo2lsnbq().s[89]++;
    setStoredValue(initialValue);
    /* istanbul ignore next */
    cov_26jo2lsnbq().s[90]++;
    setIsLoading(false);
  }, [key, readValue]);
  /* istanbul ignore next */
  cov_26jo2lsnbq().s[91]++;
  return {
    value: storedValue,
    setValue,
    removeValue,
    isSupported,
    isLoading,
    error
  };
