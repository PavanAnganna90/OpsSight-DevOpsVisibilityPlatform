const _theme = require("../theme");
describe('Theme Utilities', ()=>{
    let mockDocumentElement;
    beforeEach(()=>{
        jest.clearAllMocks();
        // Create mock element with setProperty method
        mockDocumentElement = {
            style: {
                setProperty: jest.fn(),
                removeProperty: jest.fn(),
                getPropertyValue: jest.fn()
            },
            classList: {
                add: jest.fn(),
                remove: jest.fn(),
                contains: jest.fn(),
                toggle: jest.fn()
            }
        };
        // Mock document.documentElement
        Object.defineProperty(document, 'documentElement', {
            value: mockDocumentElement,
            writable: true,
            configurable: true
        });
        // Mock Date for dynamic theme testing
        jest.useFakeTimers();
    });
    afterEach(()=>{
        jest.useRealTimers();
    });
    describe('Theme Tokens', ()=>{
        it('should export motion tokens', ()=>{
            expect(_theme.motionTokens).toHaveProperty('duration');
            expect(_theme.motionTokens).toHaveProperty('easing');
            expect(_theme.motionTokens.duration).toHaveProperty('instant');
            expect(_theme.motionTokens.duration).toHaveProperty('fast');
            expect(_theme.motionTokens.duration).toHaveProperty('normal');
            expect(_theme.motionTokens.duration).toHaveProperty('slow');
            expect(_theme.motionTokens.duration).toHaveProperty('slower');
            expect(_theme.motionTokens.easing).toHaveProperty('bounce');
            expect(_theme.motionTokens.easing).toHaveProperty('smooth');
            expect(_theme.motionTokens.easing).toHaveProperty('gentle');
            expect(_theme.motionTokens.easing).toHaveProperty('spring');
        });
        it('should export spacing tokens', ()=>{
            expect('px' in _theme.spacingTokens).toBe(true);
            expect('0.5' in _theme.spacingTokens).toBe(true);
            expect('1' in _theme.spacingTokens).toBe(true);
            expect('64' in _theme.spacingTokens).toBe(true);
            expect(_theme.spacingTokens.px).toBe('1px');
            expect(_theme.spacingTokens['0.5']).toBe('0.125rem');
            expect(_theme.spacingTokens['4']).toBe('1rem');
            expect(_theme.spacingTokens['64']).toBe('16rem');
        });
        it('should export typography tokens', ()=>{
            expect(_theme.typographyTokens).toHaveProperty('fontSizes');
            expect(_theme.typographyTokens).toHaveProperty('fontWeights');
            expect(_theme.typographyTokens).toHaveProperty('letterSpacing');
            expect(_theme.typographyTokens).toHaveProperty('lineHeight');
            expect(_theme.typographyTokens.fontSizes.base).toBe('1rem');
            expect(_theme.typographyTokens.fontWeights.normal).toBe('400');
            expect(_theme.typographyTokens.letterSpacing.normal).toBe('0em');
            expect(_theme.typographyTokens.lineHeight.normal).toBe('1.5');
        });
        it('should export shadow tokens', ()=>{
            expect(_theme.shadowTokens).toHaveProperty('sm');
            expect(_theme.shadowTokens).toHaveProperty('base');
            expect(_theme.shadowTokens).toHaveProperty('md');
            expect(_theme.shadowTokens).toHaveProperty('lg');
            expect(_theme.shadowTokens).toHaveProperty('xl');
            expect(_theme.shadowTokens).toHaveProperty('2xl');
            expect(_theme.shadowTokens).toHaveProperty('inner');
            expect(typeof _theme.shadowTokens.sm).toBe('string');
            expect(_theme.shadowTokens.sm).toContain('rgb');
        });
    });
    describe('getThemeVariables', ()=>{
        it('should return theme variables for minimal light theme', ()=>{
            const config = {
                variant: 'minimal',
                colorMode: 'light',
                reducedMotion: false
            };
            const variables = (0, _theme.getThemeVariables)(config);
            expect(variables).toHaveProperty('--bg-primary');
            expect(variables).toHaveProperty('--bg-secondary');
            expect(variables).toHaveProperty('--text-primary');
            expect(variables).toHaveProperty('--text-secondary');
            expect(variables).toHaveProperty('--border');
            expect(variables).toHaveProperty('--accent');
            expect(variables).toHaveProperty('--accent-hover');
            expect(typeof variables['--bg-primary']).toBe('string');
            expect(typeof variables['--text-primary']).toBe('string');
        });
        it('should return theme variables for minimal dark theme', ()=>{
            const config = {
                variant: 'minimal',
                colorMode: 'dark',
                reducedMotion: false
            };
            const variables = (0, _theme.getThemeVariables)(config);
            expect(variables).toHaveProperty('--bg-primary');
            expect(variables).toHaveProperty('--text-primary');
            // Dark theme should have different colors than light
            const lightConfig = {
                ...config,
                colorMode: 'light'
            };
            const lightVariables = (0, _theme.getThemeVariables)(lightConfig);
            expect(variables['--bg-primary']).not.toBe(lightVariables['--bg-primary']);
        });
        it('should handle all theme variants', ()=>{
            const variants = [
                'minimal',
                'neo-brutalist',
                'glassmorphic',
                'cyberpunk',
                'editorial',
                'accessible',
                'dynamic'
            ];
            variants.forEach((variant)=>{
                const config = {
                    variant,
                    colorMode: 'light',
                    reducedMotion: false
                };
                const variables = (0, _theme.getThemeVariables)(config);
                expect(variables).toHaveProperty('--bg-primary');
                expect(variables).toHaveProperty('--text-primary');
            });
        });
        it('should handle all color modes', ()=>{
            const colorModes = [
                'light',
                'dark',
                'high-contrast',
                'system'
            ];
            colorModes.forEach((colorMode)=>{
                const config = {
                    variant: 'minimal',
                    colorMode,
                    reducedMotion: false
                };
                const variables = (0, _theme.getThemeVariables)(config);
                expect(variables).toHaveProperty('--bg-primary');
                expect(variables).toHaveProperty('--text-primary');
            });
        });
        it('should handle contextual themes', ()=>{
            const contexts = [
                'default',
                'focus',
                'relax',
                'energize'
            ];
            contexts.forEach((context)=>{
                const config = {
                    variant: 'minimal',
                    colorMode: 'light',
                    context,
                    reducedMotion: false
                };
                const variables = (0, _theme.getThemeVariables)(config);
                expect(variables).toHaveProperty('--bg-primary');
                expect(variables).toHaveProperty('--text-primary');
            });
        });
        it('should handle reduced motion preference', ()=>{
            const config = {
                variant: 'minimal',
                colorMode: 'light',
                reducedMotion: true
            };
            const variables = (0, _theme.getThemeVariables)(config);
            expect(variables).toHaveProperty('--bg-primary');
            // Should still return valid theme variables
            expect(typeof variables['--bg-primary']).toBe('string');
        });
        it('should handle system color mode', ()=>{
            // Mock matchMedia for system preference
            const mockMatchMedia = jest.fn().mockImplementation((query)=>({
                    matches: query.includes('prefers-color-scheme: dark'),
                    media: query,
                    onchange: null,
                    addListener: jest.fn(),
                    removeListener: jest.fn(),
                    addEventListener: jest.fn(),
                    removeEventListener: jest.fn(),
                    dispatchEvent: jest.fn()
                }));
            Object.defineProperty(window, 'matchMedia', {
                value: mockMatchMedia,
                writable: true
            });
            const config = {
                variant: 'minimal',
                colorMode: 'system',
                reducedMotion: false
            };
            const variables = (0, _theme.getThemeVariables)(config);
            expect(variables).toHaveProperty('--bg-primary');
            expect(mockMatchMedia).toHaveBeenCalledWith('(prefers-color-scheme: dark)');
        });
    });
    describe('Dynamic Theme', ()=>{
        it('should return morning theme during morning hours', ()=>{
            // Set time to 9 AM
            jest.setSystemTime(new Date('2023-01-01T09:00:00'));
            const config = {
                variant: 'dynamic',
                colorMode: 'light',
                reducedMotion: false
            };
            const variables = (0, _theme.getThemeVariables)(config);
            expect(variables).toHaveProperty('--bg-primary');
