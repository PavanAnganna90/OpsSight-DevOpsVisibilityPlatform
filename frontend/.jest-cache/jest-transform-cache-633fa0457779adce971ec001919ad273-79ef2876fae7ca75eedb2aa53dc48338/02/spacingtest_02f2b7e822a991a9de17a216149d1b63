/**
 * Unit tests for spacing tokens
 * Tests spacing token generation, validation, and utility functions
 */ "use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
const _spacing = require("../../styles/tokens/spacing");
describe('Spacing Tokens', ()=>{
    describe('spacing scale', ()=>{
        // Expected use test
        it('should have complete spacing scale', ()=>{
            expect(_spacing.spacing[0]).toBe('0');
            expect(_spacing.spacing.px).toBe('1px');
            expect(_spacing.spacing[4]).toBe('1rem');
            expect(_spacing.spacing[96]).toBe('24rem');
        });
        // Edge case test
        it('should use consistent rem units except for pixel and zero values', ()=>{
            Object.entries(_spacing.spacing).forEach(([key, value])=>{
                if (key === '0') {
                    expect(value).toBe('0');
                } else if (key === 'px') {
                    expect(value).toBe('1px');
                } else {
                    expect(value).toMatch(/rem$/);
                }
            });
        });
        // Expected use test
        it('should have proper mathematical progression', ()=>{
            expect(_spacing.spacing[1]).toBe('0.25rem'); // 4px
            expect(_spacing.spacing[2]).toBe('0.5rem'); // 8px
            expect(_spacing.spacing[4]).toBe('1rem'); // 16px
            expect(_spacing.spacing[8]).toBe('2rem'); // 32px
        });
    });
    describe('containers', ()=>{
        // Expected use test
        it('should have all required container sizes', ()=>{
            expect(_spacing.containers).toHaveProperty('sm');
            expect(_spacing.containers).toHaveProperty('md');
            expect(_spacing.containers).toHaveProperty('lg');
            expect(_spacing.containers).toHaveProperty('xl');
            expect(_spacing.containers).toHaveProperty('2xl');
        });
        // Edge case test
        it('should have increasing sizes', ()=>{
            const sizes = Object.values(_spacing.containers);
            const numericSizes = sizes.map((size)=>parseFloat(size));
            for(let i = 1; i < numericSizes.length; i++){
                expect(numericSizes[i]).toBeGreaterThan(numericSizes[i - 1]);
            }
        });
    });
    describe('breakpoints', ()=>{
        // Expected use test
        it('should have all required breakpoints', ()=>{
            expect(_spacing.breakpoints).toHaveProperty('sm');
            expect(_spacing.breakpoints).toHaveProperty('md');
            expect(_spacing.breakpoints).toHaveProperty('lg');
            expect(_spacing.breakpoints).toHaveProperty('xl');
            expect(_spacing.breakpoints).toHaveProperty('2xl');
        });
        // Expected use test
        it('should use pixel units for media queries', ()=>{
            Object.values(_spacing.breakpoints).forEach((breakpoint)=>{
                expect(breakpoint).toMatch(/px$/);
            });
        });
        // Edge case test
        it('should have increasing breakpoint values', ()=>{
            const values = Object.values(_spacing.breakpoints);
            const numericValues = values.map((bp)=>parseInt(bp));
            for(let i = 1; i < numericValues.length; i++){
                expect(numericValues[i]).toBeGreaterThan(numericValues[i - 1]);
            }
        });
    });
    describe('borderRadius', ()=>{
        // Expected use test
        it('should have complete radius scale', ()=>{
            expect(_spacing.borderRadius.none).toBe('0');
            expect(_spacing.borderRadius.sm).toBe('0.125rem');
            expect(_spacing.borderRadius.full).toBe('9999px');
        });
        // Edge case test
        it('should handle special values', ()=>{
            expect(_spacing.borderRadius.none).toBe('0');
            expect(_spacing.borderRadius.full).toBe('9999px');
        });
    });
    describe('borderWidth', ()=>{
        // Expected use test
        it('should have pixel-based widths', ()=>{
            expect(_spacing.borderWidth[0]).toBe('0');
            expect(_spacing.borderWidth[1]).toBe('1px');
            expect(_spacing.borderWidth[2]).toBe('2px');
            expect(_spacing.borderWidth[8]).toBe('8px');
        });
        // Edge case test
        it('should have zero as string or number', ()=>{
            expect(_spacing.borderWidth[0]).toBe('0');
        });
    });
    describe('shadows', ()=>{
        // Expected use test
        it('should have complete shadow scale', ()=>{
            expect(_spacing.shadows.none).toBe('none');
            expect(_spacing.shadows.sm).toContain('rgb');
            expect(_spacing.shadows['2xl']).toContain('rgb');
        });
        // Expected use test
        it('should use modern RGB syntax with alpha', ()=>{
            Object.entries(_spacing.shadows).forEach(([key, value])=>{
                if (key !== 'none') {
                    expect(value).toMatch(/rgb\(\d+ \d+ \d+ \/ 0\.\d+\)/);
                }
            });
        });
        // Edge case test
        it('should handle inner shadows', ()=>{
            expect(_spacing.shadows.inner).toContain('inset');
        });
    });
    describe('zIndex', ()=>{
        // Expected use test
        it('should have layering hierarchy', ()=>{
            expect(Number(_spacing.zIndex.dropdown)).toBeLessThan(Number(_spacing.zIndex.modal));
            expect(Number(_spacing.zIndex.modal)).toBeLessThan(Number(_spacing.zIndex.popover));
            expect(Number(_spacing.zIndex.popover)).toBeLessThan(Number(_spacing.zIndex.tooltip));
            expect(Number(_spacing.zIndex.tooltip)).toBeLessThan(Number(_spacing.zIndex.notification));
        });
        // Expected use test
        it('should have special values', ()=>{
            expect(_spacing.zIndex.auto).toBe('auto');
            expect(_spacing.zIndex[0]).toBe('0'); // Values are strings in CSS
        });
    });
    describe('opacity', ()=>{
        // Expected use test
        it('should have complete opacity scale', ()=>{
            expect(_spacing.opacity[0]).toBe('0'); // Values are strings in CSS
            expect(_spacing.opacity[50]).toBe('0.5');
            expect(_spacing.opacity[100]).toBe('1');
        });
        // Edge case test
        it('should have values between 0 and 1', ()=>{
            Object.values(_spacing.opacity).forEach((value)=>{
                const numValue = parseFloat(value);
                expect(numValue).toBeGreaterThanOrEqual(0);
                expect(numValue).toBeLessThanOrEqual(1);
            });
        });
    });
    describe('transitions', ()=>{
        // Expected use test
        it('should have basic transition types', ()=>{
            expect(_spacing.transitions.none).toBe('none');
            expect(_spacing.transitions.all).toBe('all');
            expect(_spacing.transitions.default).toContain('cubic-bezier');
        });
        // Expected use test
        it('should have property-specific transitions', ()=>{
            expect(_spacing.transitions.colors).toContain('color');
            expect(_spacing.transitions.colors).toContain('background-color');
            expect(_spacing.transitions.opacity).toContain('opacity');
            expect(_spacing.transitions.transform).toContain('transform');
        });
        // Edge case test
        it('should use consistent easing functions', ()=>{
            const easingPattern = /cubic-bezier\([\d.,\s]+\)/;
            Object.entries(_spacing.transitions).forEach(([key, value])=>{
                if (key !== 'none' && key !== 'all') {
                    expect(value).toMatch(easingPattern);
                }
            });
        });
    });
    describe('generateSpacingVariables', ()=>{
        // Expected use test
        it('should generate CSS custom properties', ()=>{
            const variables = (0, _spacing.generateSpacingVariables)();
            expect(variables).toHaveProperty('--spacing-0');
            expect(variables).toHaveProperty('--spacing-4');
            expect(variables).toHaveProperty('--border-radius-base');
            expect(variables).toHaveProperty('--shadow-base');
        });
        // Expected use test
        it('should handle decimal spacing keys correctly', ()=>{
            const variables = (0, _spacing.generateSpacingVariables)();
            // The actual keys use dots, not underscores
            // Use direct property access instead of toHaveProperty due to Jest issues with dot notation
            expect(variables['--spacing-0.5']).toBeDefined();
            expect(variables['--spacing-1.5']).toBeDefined();
            expect(variables['--spacing-2.5']).toBeDefined();
            // Also check the values are correct
            expect(variables['--spacing-0.5']).toBe('0.125rem');
            expect(variables['--spacing-1.5']).toBe('0.375rem');
            expect(variables['--spacing-2.5']).toBe('0.625rem');
        });
        // Edge case test
        it('should preserve special values', ()=>{
            const variables = (0, _spacing.generateSpacingVariables)();
            expect(variables['--border-radius-full']).toBe('9999px');
            expect(variables['--shadow-none']).toBe('none');
            expect(variables['--z-index-auto']).toBe('auto');
        });
        // Failure case test
        it('should handle empty objects gracefully', ()=>{
            // Mock empty spacing
            const originalSpacing = _spacing.spacing;
            Object.keys(_spacing.spacing).forEach((key)=>{
                delete _spacing.spacing[key];
            });
            const variables = (0, _spacing.generateSpacingVariables)();
            expect(typeof variables).toBe('object');
            // Restore original
            Object.assign(_spacing.spacing, originalSpacing);
        });
    });
    describe('defaultSpacingConfig', ()=>{
        // Expected use test
        it('should have variables property', ()=>{
            expect(_spacing.defaultSpacingConfig).toHaveProperty('variables');
            expect(typeof _spacing.defaultSpacingConfig.variables).toBe('object');
        });
        // Expected use test
        it('should contain all spacing-related variables', ()=>{
            const { variables } = _spacing.defaultSpacingConfig;
            // Check various categories exist
            expect(Object.keys(variables).some((key)=>key.includes('spacing'))).toBe(true);
            expect(Object.keys(variables).some((key)=>key.includes('border-radius'))).toBe(true);
            expect(Object.keys(variables).some((key)=>key.includes('shadow'))).toBe(true);
            expect(Object.keys(variables).some((key)=>key.includes('z-index'))).toBe(true);
        });
        // Edge case test
        it('should have consistent variable naming convention', ()=>{
            const { variables } = _spacing.defaultSpacingConfig;
            Object.keys(variables).forEach((key)=>{
                expect(key).toMatch(/^--/);
                expect(key).toMatch(/^--(spacing|container|border|shadow|z-index|opacity|transition|duration|easing)/);
            });
        });
    });
    describe('integration tests', ()=>{
        // Expected use test
        it('should have consistent spacing relationships', ()=>{
            // Check that spacing values make sense relative to each other
            const spacing4 = parseFloat(_spacing.spacing[4]); // 1rem
            const spacing8 = parseFloat(_spacing.spacing[8]); // 2rem
            expect(spacing8).toBe(spacing4 * 2);
        });
        // Edge case test
        it('should have breakpoints that work with container sizes', ()=>{
            // Container sizes should be smaller than their corresponding breakpoints
            const containerSm = parseFloat(_spacing.containers.sm);
            const breakpointSm = parseInt(_spacing.breakpoints.sm);
            expect(containerSm * 16).toBeLessThan(breakpointSm); // Convert rem to px
        });
        // Expected use test
        it('should have shadow hierarchy that makes visual sense', ()=>{
            // Larger shadows should have larger blur radius (rough check)
            expect(_spacing.shadows.sm.length).toBeLessThan(_spacing.shadows.lg.length);
            // Note: 2xl shadow might be shorter due to different structure
            expect(_spacing.shadows.lg.length).toBeGreaterThan(0);
            expect(_spacing.shadows['2xl'].length).toBeGreaterThan(0);
        });
    });
});

