function _interop_require_wildcard(obj, nodeInterop) {
  /* istanbul ignore next */
  cov_2aoa6g202t().f[3]++;
  cov_2aoa6g202t().s[12]++;
  if (
  /* istanbul ignore next */
  (cov_2aoa6g202t().b[3][0]++, !nodeInterop) &&
