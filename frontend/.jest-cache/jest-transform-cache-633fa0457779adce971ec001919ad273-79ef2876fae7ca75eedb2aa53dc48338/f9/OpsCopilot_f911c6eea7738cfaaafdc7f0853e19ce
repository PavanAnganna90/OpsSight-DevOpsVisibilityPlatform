  /* istanbul ignore next */
  cov_2aoa6g202t().s[17]++;
  if (
  /* istanbul ignore next */
  (cov_2aoa6g202t().b[7][0]++, cache) &&
  /* istanbul ignore next */
  (cov_2aoa6g202t().b[7][1]++, cache.has(obj))) {
    /* istanbul ignore next */
    cov_2aoa6g202t().b[6][0]++;
    cov_2aoa6g202t().s[18]++;
    return cache.get(obj);
  } else
  /* istanbul ignore next */
  {
    cov_2aoa6g202t().b[6][1]++;
  }
  var newObj =
  /* istanbul ignore next */
  (cov_2aoa6g202t().s[19]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
