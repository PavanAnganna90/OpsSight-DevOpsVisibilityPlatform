                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                className: "space-y-2",
                                                children: pod.events.slice(0, 5).map((event, index)=>/*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                        className: "flex items-start space-x-3 text-sm",
                                                        children: [
                                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                                className: `w-2 h-2 rounded-full mt-1.5 ${event.type === 'Warning' ? 'bg-yellow-500' : 'bg-green-500'}`
                                                            }),
                                                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                                className: "flex-1",
                                                                children: [
