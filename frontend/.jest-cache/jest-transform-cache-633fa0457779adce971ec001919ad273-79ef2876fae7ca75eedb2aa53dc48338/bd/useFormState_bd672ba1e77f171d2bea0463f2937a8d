        (cov_10ixzfzkvs().s[45]++, validateFieldInternal(fieldName, value, newValues));
        /* istanbul ignore next */
        cov_10ixzfzkvs().s[46]++;
        if (error) {
          /* istanbul ignore next */
          cov_10ixzfzkvs().b[15][0]++;
          cov_10ixzfzkvs().s[47]++;
          newErrors[fieldName] = error;
        } else {
          /* istanbul ignore next */
          cov_10ixzfzkvs().b[15][1]++;
          cov_10ixzfzkvs().s[48]++;
          delete newErrors[fieldName];
        }
      } else
      /* istanbul ignore next */
      {
        cov_10ixzfzkvs().b[14][1]++;
      }
      cov_10ixzfzkvs().s[49]++;
      return {
        ...prevState,
        values: newValues,
        errors: newErrors,
        isDirty: true,
        isValid: Object.keys(newErrors).length === 0
      };
    });
  }, [updateState, validateFieldInternal]));
  /**
  * Validate a specific field
  */
  const validateField =
  /* istanbul ignore next */
  (cov_10ixzfzkvs().s[50]++, (0, _react.useCallback)(fieldName => {
    /* istanbul ignore next */
    cov_10ixzfzkvs().f[12]++;
    const value =
    /* istanbul ignore next */
    (cov_10ixzfzkvs().s[51]++, getValue(fieldName));
    const error =
    /* istanbul ignore next */
    (cov_10ixzfzkvs().s[52]++, validateFieldInternal(fieldName, value, currentValues));
    /* istanbul ignore next */
    cov_10ixzfzkvs().s[53]++;
    updateState(prevState => {
      /* istanbul ignore next */
      cov_10ixzfzkvs().f[13]++;
      cov_10ixzfzkvs().s[54]++;
      return {
        ...prevState,
        errors: error ?
        /* istanbul ignore next */
        (cov_10ixzfzkvs().b[16][0]++, {
          ...prevState.errors,
          [fieldName]: error
        }) :
        /* istanbul ignore next */
        (cov_10ixzfzkvs().b[16][1]++, {
          ...prevState.errors,
          [fieldName]: undefined
        }),
        isValid: error ?
        /* istanbul ignore next */
        (cov_10ixzfzkvs().b[17][0]++, false) :
        /* istanbul ignore next */
        (cov_10ixzfzkvs().b[17][1]++, Object.keys({
          ...prevState.errors,
          [fieldName]: undefined
        }).filter(key => {
          /* istanbul ignore next */
          cov_10ixzfzkvs().f[14]++;
          cov_10ixzfzkvs().s[55]++;
          return prevState.errors[key];
        }).length === 0)
      };
    });
    /* istanbul ignore next */
    cov_10ixzfzkvs().s[56]++;
    return error;
  }, [getValue, currentValues, validateFieldInternal, updateState]));
  /**
  * Validate all fields
  */
  const validateAll =
  /* istanbul ignore next */
  (cov_10ixzfzkvs().s[57]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_10ixzfzkvs().f[15]++;
    const errors =
    /* istanbul ignore next */
    (cov_10ixzfzkvs().s[58]++, {});
    /* istanbul ignore next */
    cov_10ixzfzkvs().s[59]++;
    Object.keys(fieldConfigs.current).forEach(fieldName => {
      /* istanbul ignore next */
      cov_10ixzfzkvs().f[16]++;
      const value =
      /* istanbul ignore next */
      (cov_10ixzfzkvs().s[60]++, getValue(fieldName));
      const error =
      /* istanbul ignore next */
      (cov_10ixzfzkvs().s[61]++, validateFieldInternal(fieldName, value, currentValues));
      /* istanbul ignore next */
      cov_10ixzfzkvs().s[62]++;
      if (error) {
        /* istanbul ignore next */
        cov_10ixzfzkvs().b[18][0]++;
        cov_10ixzfzkvs().s[63]++;
        errors[fieldName] = error;
      } else
      /* istanbul ignore next */
      {
        cov_10ixzfzkvs().b[18][1]++;
      }
    });
    /* istanbul ignore next */
    cov_10ixzfzkvs().s[64]++;
    updateState(prevState => {
      /* istanbul ignore next */
      cov_10ixzfzkvs().f[17]++;
      cov_10ixzfzkvs().s[65]++;
      return {
        ...prevState,
        errors,
        isValid: Object.keys(errors).length === 0
      };
    });
    /* istanbul ignore next */
    cov_10ixzfzkvs().s[66]++;
    return Object.keys(errors).length === 0;
  }, [getValue, currentValues, validateFieldInternal, updateState]));
  /**
  * Get error for a specific field
  */
  const getError =
  /* istanbul ignore next */
  (cov_10ixzfzkvs().s[67]++, (0, _react.useCallback)(fieldName => {
    /* istanbul ignore next */
    cov_10ixzfzkvs().f[18]++;
    cov_10ixzfzkvs().s[68]++;
    return /* istanbul ignore next */(cov_10ixzfzkvs().b[19][0]++, state.errors[fieldName]) ||
    /* istanbul ignore next */
    (cov_10ixzfzkvs().b[19][1]++, null);
  }, [state.errors]));
  /**
  * Set error for a specific field
  */
  const setError =
  /* istanbul ignore next */
  (cov_10ixzfzkvs().s[69]++, (0, _react.useCallback)((fieldName, error) => {
    /* istanbul ignore next */
    cov_10ixzfzkvs().f[19]++;
    cov_10ixzfzkvs().s[70]++;
    updateState(prevState => {
      /* istanbul ignore next */
      cov_10ixzfzkvs().f[20]++;
      const newErrors =
      /* istanbul ignore next */
      (cov_10ixzfzkvs().s[71]++, {
        ...prevState.errors
      });
      /* istanbul ignore next */
      cov_10ixzfzkvs().s[72]++;
      if (error) {
        /* istanbul ignore next */
        cov_10ixzfzkvs().b[20][0]++;
        cov_10ixzfzkvs().s[73]++;
        newErrors[fieldName] = error;
      } else {
        /* istanbul ignore next */
        cov_10ixzfzkvs().b[20][1]++;
        cov_10ixzfzkvs().s[74]++;
        delete newErrors[fieldName];
      }
      /* istanbul ignore next */
      cov_10ixzfzkvs().s[75]++;
      return {
        ...prevState,
        errors: newErrors,
        isValid: Object.keys(newErrors).length === 0
      };
    });
  }, [updateState]));
  /**
  * Mark field as touched
  */
  const setTouched =
  /* istanbul ignore next */
  (cov_10ixzfzkvs().s[76]++, (0, _react.useCallback)((fieldName, touched =
  /* istanbul ignore next */
  (cov_10ixzfzkvs().b[21][0]++, true)) => {
    /* istanbul ignore next */
    cov_10ixzfzkvs().f[21]++;
    cov_10ixzfzkvs().s[77]++;
    updateState(prevState => {
      /* istanbul ignore next */
      cov_10ixzfzkvs().f[22]++;
      cov_10ixzfzkvs().s[78]++;
      return {
        ...prevState,
        touched: {
          ...prevState.touched,
          [fieldName]: touched
        }
      };
    });
    // Validate on blur if configured
    /* istanbul ignore next */
    cov_10ixzfzkvs().s[79]++;
    if (touched) {
      /* istanbul ignore next */
      cov_10ixzfzkvs().b[22][0]++;
      const config =
      /* istanbul ignore next */
      (cov_10ixzfzkvs().s[80]++, fieldConfigs.current[fieldName]);
      /* istanbul ignore next */
      cov_10ixzfzkvs().s[81]++;
      if (config?.validateOnBlur !== false) {
        /* istanbul ignore next */
        cov_10ixzfzkvs().b[23][0]++;
        cov_10ixzfzkvs().s[82]++;
        validateField(fieldName);
      } else
      /* istanbul ignore next */
      {
        cov_10ixzfzkvs().b[23][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_10ixzfzkvs().b[22][1]++;
    }
  }, [updateState, validateField]));
  /**
  * Submit the form
  */
  const handleSubmit =
  /* istanbul ignore next */
  (cov_10ixzfzkvs().s[83]++, (0, _react.useCallback)(async event => {
    /* istanbul ignore next */
    cov_10ixzfzkvs().f[23]++;
    cov_10ixzfzkvs().s[84]++;
    if (event) {
      /* istanbul ignore next */
      cov_10ixzfzkvs().b[24][0]++;
      cov_10ixzfzkvs().s[85]++;
      event.preventDefault();
    } else
    /* istanbul ignore next */
    {
      cov_10ixzfzkvs().b[24][1]++;
    }
    cov_10ixzfzkvs().s[86]++;
    updateState(prevState => {
      /* istanbul ignore next */
      cov_10ixzfzkvs().f[24]++;
      cov_10ixzfzkvs().s[87]++;
      return {
        ...prevState,
        isSubmitting: true,
        submitCount: prevState.submitCount + 1
      };
    });
    /* istanbul ignore next */
    cov_10ixzfzkvs().s[88]++;
    try {
      /* istanbul ignore next */
      cov_10ixzfzkvs().s[89]++;
      // Validate if required
      if (validateOnSubmit) {
        /* istanbul ignore next */
        cov_10ixzfzkvs().b[25][0]++;
        const isValid =
        /* istanbul ignore next */
        (cov_10ixzfzkvs().s[90]++, validateAll());
        /* istanbul ignore next */
        cov_10ixzfzkvs().s[91]++;
        if (!isValid) {
          /* istanbul ignore next */
          cov_10ixzfzkvs().b[26][0]++;
          cov_10ixzfzkvs().s[92]++;
          updateState(prevState => {
            /* istanbul ignore next */
            cov_10ixzfzkvs().f[25]++;
            cov_10ixzfzkvs().s[93]++;
            return {
              ...prevState,
              isSubmitting: false
            };
          });
          /* istanbul ignore next */
          cov_10ixzfzkvs().s[94]++;
          return;
        } else
        /* istanbul ignore next */
        {
          cov_10ixzfzkvs().b[26][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_10ixzfzkvs().b[25][1]++;
      }
      // Call submit handler
      cov_10ixzfzkvs().s[95]++;
      if (onSubmit) {
        /* istanbul ignore next */
        cov_10ixzfzkvs().b[27][0]++;
        cov_10ixzfzkvs().s[96]++;
        await onSubmit(currentValues);
      } else
      /* istanbul ignore next */
      {
        cov_10ixzfzkvs().b[27][1]++;
      }
      // Reset form if configured
      cov_10ixzfzkvs().s[97]++;
      if (resetOnSubmit) {
        /* istanbul ignore next */
        cov_10ixzfzkvs().b[28][0]++;
        cov_10ixzfzkvs().s[98]++;
        reset();
      } else
      /* istanbul ignore next */
      {
        cov_10ixzfzkvs().b[28][1]++;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_10ixzfzkvs().s[99]++;
      if (onSubmitError) {
        /* istanbul ignore next */
        cov_10ixzfzkvs().b[29][0]++;
        cov_10ixzfzkvs().s[100]++;
        onSubmitError(error);
      } else
      /* istanbul ignore next */
      {
        cov_10ixzfzkvs().b[29][1]++;
      }
      cov_10ixzfzkvs().s[101]++;
      console.error('Form submission error:', error);
    } finally {
      /* istanbul ignore next */
      cov_10ixzfzkvs().s[102]++;
      updateState(prevState => {
        /* istanbul ignore next */
        cov_10ixzfzkvs().f[26]++;
        cov_10ixzfzkvs().s[103]++;
        return {
          ...prevState,
          isSubmitting: false
        };
      });
    }
  }, [validateOnSubmit, validateAll, onSubmit, currentValues, resetOnSubmit, onSubmitError, updateState]));
  /**
  * Reset form to initial state
  */
  const reset =
  /* istanbul ignore next */
  (cov_10ixzfzkvs().s[104]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_10ixzfzkvs().f[27]++;
    cov_10ixzfzkvs().s[105]++;
    setState(initialState);
    /* istanbul ignore next */
    cov_10ixzfzkvs().s[106]++;
    if (persistState) {
      /* istanbul ignore next */
      cov_10ixzfzkvs().b[30][0]++;
      cov_10ixzfzkvs().s[107]++;
      setPersistedValues({});
    } else
    /* istanbul ignore next */
    {
      cov_10ixzfzkvs().b[30][1]++;
    }
  }, [persistState, setPersistedValues]));
  /**
  * Clear all errors
  */
  const clearErrors =
  /* istanbul ignore next */
  (cov_10ixzfzkvs().s[108]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_10ixzfzkvs().f[28]++;
    cov_10ixzfzkvs().s[109]++;
    updateState(prevState => {
      /* istanbul ignore next */
      cov_10ixzfzkvs().f[29]++;
      cov_10ixzfzkvs().s[110]++;
      return {
        ...prevState,
        errors: {},
        isValid: true
      };
    });
  }, [updateState]));
  /**
  * Check if a field is valid
  */
  const isFieldValid =
  /* istanbul ignore next */
  (cov_10ixzfzkvs().s[111]++, (0, _react.useCallback)(fieldName => {
    /* istanbul ignore next */
    cov_10ixzfzkvs().f[30]++;
    cov_10ixzfzkvs().s[112]++;
    return !state.errors[fieldName];
  }, [state.errors]));
  /**
  * Check if field has been touched
  */
  const isFieldTouched =
  /* istanbul ignore next */
  (cov_10ixzfzkvs().s[113]++, (0, _react.useCallback)(fieldName => {
    /* istanbul ignore next */
    cov_10ixzfzkvs().f[31]++;
    cov_10ixzfzkvs().s[114]++;
    return !!state.touched[fieldName];
  }, [state.touched]));
  /* istanbul ignore next */
  cov_10ixzfzkvs().s[115]++;
  return {
    state: {
      ...state,
      values: currentValues
    },
    getValue,
    setValue,
    getError,
    setError,
    setTouched,
    validateField,
    validateAll,
    handleSubmit,
    reset,
    clearErrors,
    isFieldValid,
    isFieldTouched,
    registerField,
    unregisterField
  };
}
