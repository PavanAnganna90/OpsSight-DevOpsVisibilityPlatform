function RoleBadgeList({ roles, maxDisplay = 3, size = 'md', showIcon = true, variant = 'default' }) {
    const sortedRoles = [
        ...roles
    ].sort((a, b)=>b.priority - a.priority);
    const displayRoles = sortedRoles.slice(0, maxDisplay);
    const remainingCount = sortedRoles.length - maxDisplay;
    return /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
        className: "flex flex-wrap gap-2",
        children: [
            displayRoles.map((role)=>/*#__PURE__*/ (0, _jsxruntime.jsx)(RoleBadge, {
                    role: role,
                    size: size,
                    showIcon: showIcon,
                    variant: variant
                }, role.id)),
            remainingCount > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                className: "inline-flex items-center px-2 py-1 text-xs font-medium text-gray-500 bg-gray-500/10 border border-gray-500/20 rounded-md",
                children: [
                    "+",
                    remainingCount,
                    " more"
                ]
            })
        ]
    });
}
function UserRoleDisplay({ user, size = 'md', showIcon = true, variant = 'default' }) {
    if (user.is_superuser) {
        return /*#__PURE__*/ (0, _jsxruntime.jsx)(RoleBadge, {
