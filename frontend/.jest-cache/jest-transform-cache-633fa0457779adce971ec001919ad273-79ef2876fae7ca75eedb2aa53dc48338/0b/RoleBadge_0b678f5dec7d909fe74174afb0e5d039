        return /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
            className: "inline-flex items-center px-2 py-1 text-xs font-medium text-gray-500 bg-gray-500/10 border border-gray-500/20 rounded-md",
            children: "No roles assigned"
        });
    }
    return /*#__PURE__*/ (0, _jsxruntime.jsx)(RoleBadgeList, {
        roles: user.roles,
