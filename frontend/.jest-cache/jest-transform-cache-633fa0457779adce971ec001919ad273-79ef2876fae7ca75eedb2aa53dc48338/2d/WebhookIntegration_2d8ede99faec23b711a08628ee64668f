                                    className: "w-full px-3 py-2 border border-gray-300 dark:border-gray-600 rounded-lg focus:outline-none focus:ring-2 focus:ring-blue-500 bg-white dark:bg-gray-800",
                                    required: true
                                })
                            ]
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("label", {
                                    className: "block text-sm font-medium text-gray-700 dark:text-gray-300 mb-2",
                                    children: "Method"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("select", {
                                    value: formData.method,
                                    onChange: (e)=>setFormData((prev)=>({
                                                ...prev,
                                                method: e.target.value
                                            })),
                                    className: "w-full px-3 py-2 border border-gray-300 dark:border-gray-600 rounded-lg focus:outline-none focus:ring-2 focus:ring-blue-500 bg-white dark:bg-gray-800",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                            value: "POST",
                                            children: "POST"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                            value: "PUT",
                                            children: "PUT"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                            value: "PATCH",
                                            children: "PATCH"
                                        })
                                    ]
                                })
                            ]
                        })
                    ]
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("label", {
                            className: "block text-sm font-medium text-gray-700 dark:text-gray-300 mb-2",
                            children: "Alert Types"
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                            className: "grid grid-cols-2 gap-2",
                            children: alertTypeOptions.map((option)=>/*#__PURE__*/ (0, _jsxruntime.jsxs)("label", {
                                    className: "flex items-center",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("input", {
                                            type: "checkbox",
                                            checked: formData.alertTypes.includes(option.value),
                                            onChange: (e)=>{
                                                const newAlertTypes = e.target.checked ? [
                                                    ...formData.alertTypes,
                                                    option.value
                                                ] : formData.alertTypes.filter((t)=>t !== option.value);
                                                setFormData((prev)=>({
                                                        ...prev,
                                                        alertTypes: newAlertTypes
                                                    }));
                                            },
                                            className: "mr-2"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            className: "text-sm",
                                            children: option.label
                                        })
                                    ]
                                }, option.value))
                        })
                    ]
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("label", {
                            className: "block text-sm font-medium text-gray-700 dark:text-gray-300 mb-2",
                            children: "Authentication"
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("select", {
                            value: formData.authConfig?.type || 'none',
                            onChange: (e)=>setFormData((prev)=>({
                                        ...prev,
                                        authConfig: {
                                            ...prev.authConfig,
                                            type: e.target.value
                                        }
                                    })),
                            className: "w-full px-3 py-2 border border-gray-300 dark:border-gray-600 rounded-lg focus:outline-none focus:ring-2 focus:ring-blue-500 bg-white dark:bg-gray-800",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                    value: "none",
                                    children: "None"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                    value: "bearer",
                                    children: "Bearer Token"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                    value: "basic",
                                    children: "Basic Auth"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                    value: "api_key",
                                    children: "API Key"
                                })
                            ]
                        })
                    ]
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    className: "flex gap-3",
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.Button, {
                            type: "submit",
                            className: "flex-1",
                            children: webhook ? 'Update Webhook' : 'Create Webhook'
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.Button, {
                            type: "button",
                            onClick: onClose,
                            variant: "outline",
                            children: "Cancel"
                        })
                    ]
                })
            ]
        })
    });
};
const _default = WebhookIntegration;

